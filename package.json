{
  "name": "javascript-web-framework",
  "version": "0.0.1",
  "description": "前端web脚手架",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "client/**/*.{js,jsx}": [
      "eslint --fix"
    ],
    "client/**/*.less": [
      "stylelint --fix --syntax less"
    ]
  },
  "scripts": {
    "watch": "webpack --env=development --config --watch ./webpack/webpack.config.babel.js",
    "start": "webpack-dev-server --config ./webpack/webpack.config.dev.babel.js",
    "dll": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.dll.babel.js &&cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dll.babel.js",
    "build": "webpack --env=local --progress --config  ./webpack/webpack.config.babel.js",
    "build:prod": "webpack --env=prod --progress --config  ./webpack/webpack.config.babel.js",
    "build:stage": "webpack --env=stage --progress --config  ./webpack/webpack.config.babel.js",
    "build:test": "webpack --env=test --progress --config  ./webpack/webpack.config.babel.js",
    "build:dev": "webpack --env=dev --progress --config  ./webpack/webpack.config.babel.js"
  },
  "dependencies": {
    "antd": "3.6.5",
    "autoprefixer": "~7.1.1",
    "axios": "~0.18.0",
    "babel-cli": "~6.23.0",
    "babel-eslint": "~8.2.5",
    "babel-loader": "~7.1.4",
    "babel-plugin-import": "~1.8.0",
    "babel-plugin-syntax-async-functions": "~6.13.0",
    "babel-plugin-syntax-dynamic-import": "~6.18.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "babel-runtime": "~6.26.0",
    "clean-webpack-plugin": "~0.1.19",
    "css-loader": "~0.28.11",
    "es6-promise": "~4.2.5",
    "eslint": "~4.19.1",
    "eslint-config-airbnb": "~17.0.0",
    "eslint-loader": "~2.0.0",
    "eslint-plugin-import": "~2.13.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.10.0",
    "glue-redux": "~2.0.4",
    "history": "~4.7.2",
    "html-webpack-plugin": "~3.2.0",
    "less": "~3.0.4",
    "less-loader": "~4.1.0",
    "lodash": "~4.17.10",
    "mini-css-extract-plugin": "~0.4.1",
    "normalize.css": "~8.0.0",
    "optimize-css-assets-webpack-plugin": "~3.2.0",
    "postcss-flexbugs-fixes": "~3.3.1",
    "postcss-loader": "~2.1.5",
    "prop-types": "~15.6.2",
    "react": "~16.4.1",
    "react-dom": "~16.4.1",
    "react-loadable": "~5.4.0",
    "react-redux": "~5.0.7",
    "react-router": "~4.3.1",
    "react-router-dom": "~4.3.1",
    "react-router-redux": "~5.0.0-alpha.9",
    "redux": "~3.5.2",
    "source-map-loader": "~0.2.3",
    "style-loader": "~0.21.0",
    "uglifyjs-webpack-plugin": "~1.2.7",
    "webpack": "~4.15.0",
    "webpack-cli": "~2.0.4",
    "webpack-manifest-plugin": "~2.0.3"
  },
  "devDependencies": {
    "cross-env": "~5.2.0",
    "happypack": "~5.0.0",
    "husky": "~1.0.0",
    "lint-staged": "~7.3.0",
    "redux-devtools": "~3.4.1",
    "redux-devtools-dock-monitor": "~1.1.3",
    "redux-devtools-log-monitor": "~1.4.0",
    "speed-measure-webpack-plugin": "~1.2.3",
    "stylelint": "~9.5.0",
    "stylelint-config-standard": "~18.2.0",
    "webpack-dev-server": "~3.1.4"
  }
}
